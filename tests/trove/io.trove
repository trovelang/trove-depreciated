print :: fn (val : u32) {
    printf("%d", val)
}

print :: fn (val : bool) {
    if val {
        printf("true")
    }else {
        printf("false")
    }
}

print :: fn (val : string) {
    printf("%s", val)
}

println :: fn (val : u32) {
    printf("%d\n", val)
}

println :: fn (val : bool) {
    if val {
        printf("true\n")
    }else {
        printf("false\n")
    }
}

println :: fn (val : string) {
    printf("%s\n", val)
}